<apex:page standardController="Account" extensions="AccountController">



  <!-- Begin Form -->
  <apex:form >
    <apex:pageBlock title="add account" mode="edit">
     

      <!-- Fields -->
      <apex:pageBlockSection columns="3" showHeader="true" title="Membership Status">
        <apex:inputField styleClass="firstName" value="{!contact.FirstName}" required="true" />
        <apex:inputField styleClass="lastName" value="{!contact.LastName}"  required="true" />
      </apex:pageBlockSection>

      <!-- Button Section -->
      <apex:pageBlockButtons location="bottom">
        <apex:commandButton value="Add Member" action="{!save}" />
      </apex:pageBlockButtons>
    </apex:pageBlock>
  </apex:form>
<apex:form>
    <apex:commandButton value="Go to List" action="{!goToList}" />
</apex:form>
<apex:form>
    <apex:commandButton value="Go to Contact" action="{!goToContact}" />
</apex:form>
</apex:page>
